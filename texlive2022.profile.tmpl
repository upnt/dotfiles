selected_scheme scheme-minimal
TEXDIR {{ .chezmoi.homeDir }}/.texlive/2022
TEXMFCONFIG ~/.texlive2022/texmf-config
TEXMFHOME {{ .chezmoi.homeDir }}/texmf
TEXMFLOCAL {{ .chezmoi.homeDir }}/.texlive/texmf-local
